                        <TextBlock Text="Total IPC for the UK = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=UKTotal, Mode=OneWay}" FontSize="18" Foreground="White"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Total IPC for the USSR = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=USSRTotal, Mode=OneWay }" FontSize="18" Foreground="White"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Total IPC for Game = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=GameTotal, Mode=OneWay }" FontSize="18" Foreground="White"/>
                    </StackPanel>
                </StackPanel>

            </Grid>
        </utils1:ZoomBorder>
    </ScrollViewer>
</Window>
