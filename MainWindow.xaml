                              GroupName="67"
                              Content="Germany"
                              Checked="click67"/>
                        </StackPanel>
                        <StackPanel >
                            <RadioButton
                              VerticalAlignment="Top"
                              x:Name="J67"
                              GroupName="67"
                              Content="Japan"
                              Checked="click67"/>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel x:Name="Allies67" Visibility="Collapsed">
                        <StackPanel>
                            <RadioButton
                              x:Name="USSR67"
                              VerticalAlignment="Top"
                              GroupName="67"
                              Content="USSR"
                              Checked="click67"/>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>

                <!-- Totals -->
                <StackPanel x:Name="Totals" Grid.Column="3" Grid.Row="12" Margin="10" Grid.ColumnSpan="4" Grid.RowSpan="3">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Total IPC for Germany = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=GermanyTotal, Mode=OneWay}" FontSize="18" Foreground="White"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Total IPC for Japan = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=JapanTotal, Mode=OneWay}" FontSize="18" Foreground="White"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Total IPC for the USA = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=USATotal, Mode=OneWay}" FontSize="18" Foreground="White"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Total IPC for the UK = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=UKTotal, Mode=OneWay}" FontSize="18" Foreground="White"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Total IPC for the USSR = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=USSRTotal, Mode=OneWay }" FontSize="18" Foreground="White"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Total IPC for Game = " FontSize="18"/>
                        <TextBlock Text="{Binding Path=GameTotal, Mode=OneWay }" FontSize="18" Foreground="White"/>
                    </StackPanel>
                </StackPanel>

            </Grid>
        </utils1:ZoomBorder>
    </ScrollViewer>
</Window>
